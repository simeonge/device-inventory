<div class="container">
<h1>Devices</h1>

<table class="table">
  <thead>
    <tr>
      <th>OS</th>
      <th>Model</th>
      <th>Code</th>
      <th>Available?</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @statuses.each do |device| %>
      <tr>
        <td><%= device.os %></td>
        <td><%= device.model %></td>
        <td><%= device.code %></td>
	<td><%= device.available%></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
